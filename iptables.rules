*filter
:INPUT ACCEPT [0:0]
:FORWARD ACCEPT [0:0]
:OUTPUT ACCEPT [0:0]

# contest.informatik-olympiade.de
-A OUTPUT -p tcp -d 138.201.137.186 -j ACCEPT

-A OUTPUT -p tcp -d localhost -j ACCEPT
-A OUTPUT -m owner --uid-owner root -j ACCEPT

# DNS
-A OUTPUT -m owner --uid-owner systemd-resolve -j ACCEPT

-A OUTPUT -j REJECT
COMMIT
